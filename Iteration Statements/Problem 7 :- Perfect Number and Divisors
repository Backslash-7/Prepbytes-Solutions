#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    int t;
    cin>>t;
    while(t--)
    {
      int x,sum=0;
      cin>>x;
      for(int i=1; i<=(x/2);i++)
      {
        if(x%i==0)
        {
          sum = sum + i;
        }
      }
      if(x==sum)
      {
        cout<<"true"<<endl;
      }
      else
      {
        cout<<"false"<<endl;
      }
    }
    
    return 0;
  }
