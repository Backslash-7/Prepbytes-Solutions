#include <bits/stdc++.h>
  using namespace std;
  
  int main()
  {
    //write your code here
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
      cin>>arr[i];
    }
    for(int i=0; i<n;i++)
    {
      if(arr[i]%2==0)
      {
        cout<<arr[i]<<" ";
      }
    }
    cout<<endl;
    for(int i=0; i<n;i++)
    {
      if(arr[i]%2!=0)
      {
        cout<<arr[i]<<" ";
      }
    }
    
    
    return 0;
  }
