#include <bits/stdc++.h>
using namespace std;
  
  int multiply(int arr[],int n)
  {
    int mul=1;
    for(int j=0;j<n;j++)
      {
        mul*=arr[j];
      }
      return mul;
  }
  
  int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t--)
    {
      int n;
      cin>>n;
      int arr[n];
      for(int i=0;i<n;i++)
      {
        cin>>arr[i];
      }
      int res = multiply(arr,n);
      cout << res <<endl; 
      
    }
    
    return 0;
  }
