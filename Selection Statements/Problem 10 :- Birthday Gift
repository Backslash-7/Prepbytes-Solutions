#include <bits/stdc++.h>
using namespace std;
  
 int main()
  {
    //write your code here
    int t;
    cin>>t;
    while(t--)
    {
      int A, B, C;
      cin>>A>>B>>C;
      if(C==0)
      {
          if(A==B)
          {
             cout<<"YES"<<endl;
          }
          else
          {
            cout<<"NO"<<endl;
          }
      }
      else if((B%C)==A)
      {
        cout<<"YES"<<endl;
      }
      else
      {
        cout<<"NO"<<endl;
      }
    }
    
    return 0;
  }
